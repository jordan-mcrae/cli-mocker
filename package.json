  "bugs": {
    "url": "https://github.com/jordan-mcrae/cli-mocker/issues"
  },
  "homepage": "https://github.com/jordan-mcrae/cli-mocker#readme",
  "files": ["lib/**/*"],
  "devDependencies": {
    "@types/node": "^18.7.13",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.2"
  }
